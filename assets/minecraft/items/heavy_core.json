{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "fallback": {
      "type": "model",
      "model": "block/heavy_core"
    },
    "cases": [
      {
        "when": "torus:motor",
        "model": {
          "type": "model",
          "model": "item/motor"
        }
      },
      {
        "when": "torus:generator",
        "model": {
          "type": "model",
          "model": "item/generator"
        }
      },
      {
        "when": "torus:connector",
        "model": {
          "type": "model",
          "model": "item/connector"
        }
      }
    ]
  }
}
