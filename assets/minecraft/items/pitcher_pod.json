{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "fallback": {
      "type": "model",
      "model": "item/pitcher_pod"
    },
    "cases": [
      {
        "when": "torus:copper_wire",
        "model": {
          "type": "model",
          "model": "item/copper_wire"
        }
      },
      {
        "when": "torus:copper_coil",
        "model": {
          "type": "model",
          "model": "item/copper_coil"
        }
      }
    ]
  }
}
